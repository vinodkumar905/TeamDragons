  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource14</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CollectionWaste AS (
    SELECT 
        MONTH(Collection_Date) AS Month,
        SUM(CASE WHEN Collection_Category = 'Food Waste Compost' THEN Weight_in_lbs ELSE 0 END) AS TotalFoodWaste,
        SUM(CASE WHEN IsRecycled = 1 THEN Weight_in_lbs ELSE 0 END) AS TotalRecyclableWaste
    FROM dbo.RECYCLE_COLLECTION
    WHERE MONTH(Collection_Date) = @Month 
    GROUP BY MONTH(Collection_Date)
),
    
-- Calculate total landfill waste
LandfillWaste AS (
    SELECT 
        MONTH(Landfill_Date) AS Month,
        SUM(WEIGHT_IN_LBS) AS TotalLandfillWaste
    FROM dbo.Landfill
    WHERE MONTH(Landfill_Date) = @Month 
    GROUP BY MONTH(Landfill_Date)
)

-- Calculate the diversion rate
SELECT 
    c.Month,
    ISNULL(l.TotalLandfillWaste, 0) AS LandfillWaste,
    ISNULL(c.TotalFoodWaste, 0) + ISNULL(c.TotalRecyclableWaste, 0) + ISNULL(l.TotalLandfillWaste, 0) AS TotalWaste,
    c.TotalRecyclableWaste,
    CASE 
        WHEN (ISNULL(c.TotalFoodWaste, 0) + ISNULL(c.TotalRecyclableWaste, 0) + ISNULL(l.TotalLandfillWaste, 0)) &gt; 0 
        THEN CAST((ISNULL(c.TotalRecyclableWaste, 0) * 1.0 / 
                    (ISNULL(c.TotalFoodWaste, 0) + ISNULL(c.TotalRecyclableWaste, 0) + ISNULL(l.TotalLandfillWaste, 0))) * 100 AS DECIMAL(5,2))
        ELSE 0
    END AS DiversionRate
FROM CollectionWaste c
LEFT JOIN LandfillWaste l ON c.Month = l.Month
WHERE c.Month = @Month 
ORDER BY c.Month;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
