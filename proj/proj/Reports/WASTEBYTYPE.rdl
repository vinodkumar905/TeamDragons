  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource13</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    'Landfill' AS WasteType,
    SUM(WEIGHT_IN_LBS) AS TotalWeight
FROM dbo.Landfill
WHERE MONTH(Landfill_Date) = @Month

UNION ALL

SELECT 
    'Food Waste Compost' AS WasteType,
    SUM(Weight_in_lbs) AS TotalWeight
FROM dbo.Recycle_collection
WHERE COLLECTION_Category = 'Food Waste'
  AND MONTH(Collection_Date) = @Month 

UNION ALL

SELECT 
    'Recycling' AS WasteType,
    SUM(Weight_in_lbs) AS TotalWeight
FROM dbo.Recycle_collection
WHERE IsRecycled = 1
  AND MONTH(Collection_Date) = @Month;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
